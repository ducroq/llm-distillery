<?xml version="1.0" encoding="utf-8"?>
<FreeFileSync XmlType="GUI" XmlFormat="23">
    <Notes/>
    <Compare>
        <Variant>TimeAndSize</Variant>
        <Symlinks>Follow</Symlinks>
        <IgnoreTimeShift/>
    </Compare>
    <Synchronize>
        <Changes>
            <Left Create="right" Update="right" Delete="right"/>
            <Right Create="left" Update="left" Delete="left"/>
        </Changes>
        <DeletionPolicy>Versioning</DeletionPolicy>
        <VersioningFolder Style="Replace">C:\local_dev\llm-distillery\.sync-backups</VersioningFolder>
    </Synchronize>
    <Filter>
        <Include>
            <Item>*</Item>
        </Include>
        <Exclude>
            <Item>\.git\</Item>
            <Item>\venv\</Item>
            <Item>\env\</Item>
            <Item>\ENV\</Item>
            <Item>\.venv\</Item>
            <Item>\venv_*\</Item>
            <Item>\*_venv\</Item>
            <Item>\__pycache__\</Item>
            <Item>*.pyc</Item>
            <Item>*.pyo</Item>
            <Item>*.pyd</Item>
            <Item>\.Python</Item>
            <Item>\build\</Item>
            <Item>\dist\</Item>
            <Item>\*.egg-info\</Item>
            <Item>\.pytest_cache\</Item>
            <Item>\.coverage</Item>
            <Item>\htmlcov\</Item>
            <Item>\.ipynb_checkpoints\</Item>
            <Item>\.vscode\</Item>
            <Item>\.idea\</Item>
            <Item>*.swp</Item>
            <Item>*.swo</Item>
            <Item>.DS_Store</Item>
            <Item>Thumbs.db</Item>
            <Item>desktop.ini</Item>
            <Item>*.bak</Item>
            <Item>*.old</Item>
            <Item>*_backup.*</Item>
            <Item>*~</Item>
            <Item>nul</Item>
            <Item>*.tmp</Item>
            <Item>\tmp\</Item>
            <Item>\temp\</Item>
            <Item>\.sync-backups\</Item>
            <Item>*\nul</Item>
        </Exclude>
        <SizeMin Unit="None">0</SizeMin>
        <SizeMax Unit="None">0</SizeMax>
        <TimeSpan Type="None">0</TimeSpan>
    </Filter>
    <FolderPairs>
        <Pair>
            <Left>C:\local_dev\llm-distillery\</Left>
            <Right>sftp://jeroen@llm-distiller/home/jeroen/llm-distillery|pwprompt</Right>
        </Pair>
    </FolderPairs>
    <Errors Ignore="false" Retry="0" Delay="5"/>
    <PostSyncCommand Condition="Completion"/>
    <LogFolder/>
    <EmailNotification Condition="Always"/>
    <GridViewType>Action</GridViewType>
</FreeFileSync>
